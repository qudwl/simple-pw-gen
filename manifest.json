{
    "name": "Password Generator",
    "version": "1.0.1",
    "description": "Simple password generator",
    "manifest_version": 3,
    "author": "Sampurna Chapagain",
    "action": {
        "default_popup": "index.html",
        "default_title": "Password Generator"
    },
    "permissions": [
        "clipboardWrite"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    }
}